{
  "name": "entityforge",
  "description": "Entities >> Objects",
  "version": "0.0.1-alpha.1",
  "homepage": "https://github.com/ggranum/entity-forge",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggranum/entity-forge.git"
  },
  "bugs": {
    "url": "https://github.com/ggranum/entity-forge/issues"
  },
  "main": "entity-forge.js",
  "keywords": [
    "utilities",
    "types",
    "validate",
    "validation",
    "generate",
    "entities",
    "objects"
  ],
  "license": "MIT",
  "private": false,
  "author": "Geoff M. Granum <geoff.granum@gmail.com> (https://geoffgranum.com)",
  "contributors": [
    "Geoff M. Granum <geoff.granum@gmail.com>"
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "connect": "^3.4.1",
    "del": "^2.x.x",
    "git-rev": "^0.2.1",
    "gulp": "^3.9.x",
    "gulp-concat": "~2.6.x",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-sourcemaps": "^1.6.0",
    "http-proxy": "^1.11.3",
    "jasmine-core": "^2.4.1",
    "minimist": "^1.1.1",
    "open": "0.0.5",
    "proxy-middleware": "^0.x.x",
    "serve-index": "^1.6.3",
    "serve-static": "^1.x.x",
    "typescript": "^1.8.10"
  },
  "scripts": {
    "build.serve": "gulp serve || true",
    "build.watch": "gulp clean && gulp build && gulp serve || true",
    "build.test": "gulp clean && gulp build && gulp serve --open=/entity-forge-test.html",
    "jspm.dist":" jspm build src/entity-forge.ts dist/entityforge.umd.js --format umd --global-name EntityForge --minify --skip-source-maps",
    "jspm.test":" jspm build src/entity-forge-test.ts  dist/entity-forge-test.js --format umd --global-name EntityForgeTest",
    "docs": "gulp clean && gulp build && gulp serve --open=/build/docs/index.html"
  },
  "dependencies": {
    "es6-shim": ">=0.22.0",
    "seedrandom": "^2.4.2",
    "systemjs": "^0.19.32"
  },
  "jspm": {
    "name": "entityforge",
    "main": "entity-forge.js",
    "dependencies": {},
    "devDependencies": {
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "jasmine-core": "npm:jasmine-core@^2.4.1",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "plugin-typescript": "github:frankwallis/plugin-typescript@^5.0.8",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "overrides": {
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:typescript@2.0.0": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
}
